#Create an empty list
my_list = []

#Append elements
my_list.append(10)
my_list.append(20)
my_list.append(30)
my_list.append(40)

#Insert 15 at index 1
my_list.insert(1, 15)

#Extend my_list
my_list.extend([50,60, 70])

#Remove last element(index-1)
del my_list[-1]

#Sort my_list
my_list.sort()

# Print the index of the value
print(my_list.index(30))

